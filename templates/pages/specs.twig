{% extends "layout.twig" %}
{% set title = "Specs" %}
{% set semiActiveClass = 'pricing' %}

{% set javascripts %}
<script>
    $(function() {
        $.setSemiActiveClass('nav_pricing');
        $.setActiveClass('nav_specs');
        $('a[href^="#"]').on('click',function (e) {
            e.preventDefault();

            var target = this.hash;
            var $target = $(target);

            $('html, body').stop().animate({
                'scrollTop': $target.offset().top
            }, 400, 'swing', function () {
                window.location.hash = target;
            });
        });


        var refs = {};
        $('#x-references li').each(function(num) {
            var id = $(this).attr('id');
            if (id) refs['#' + $(this).attr('id')] = num + 1;
        });
        console.log(refs);
        $('.reflink').each(function() {
            var ref = $(this).attr('href');
            if (ref.match(/^#ref\-.+/) && refs[ref] !== undefined) {
                $(this).text(refs[ref]);
            } else {
                $(this).text('X');
            }
        })
    });
</script>
{% endset %}



