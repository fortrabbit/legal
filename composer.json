{
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:ukautz/frbit-product-definition.git"
        }
    ],
    "require": {
        "michelf/php-markdown": ">=1.4,<1.5",
        "slim/views": "*",
        "slimcontroller/slimcontroller": ">=0.4,<0.5",
        "twig/extensions": "*",
        "twig/twig": ">=1,<2",
        "frbit/product-definition": "dev-master",
        "illuminate/cache": ">=5,<6",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "Frbit\\WwwApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Frbit\\Tests\\DocsApp": "tests/"
        }
    }
}
